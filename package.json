{
  "name": "web-tools",
  "version": "1.0.0",
  "description": "A collection of single-page web tools deployable as Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev:exif": "cd workers/exif-remover && wrangler dev",
    "dev:flowdown": "cd workers/flowdown-config && wrangler dev",
    "dev:hdr": "cd workers/hdr-tool && wrangler dev",
    "deploy:exif": "cd workers/exif-remover && wrangler deploy",
    "deploy:flowdown": "cd workers/flowdown-config && wrangler deploy",
    "deploy:hdr": "cd workers/hdr-tool && wrangler deploy",
    "deploy:all": "npm run deploy:exif && npm run deploy:flowdown && npm run deploy:hdr"
  },
  "devDependencies": {
    "wrangler": "^4.0.0"
  },
  "keywords": [
    "cloudflare-workers",
    "web-tools",
    "single-page-app"
  ],
  "author": "",
  "license": "MIT"
}
